{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "Nexus.Ingest": "Information"
    }
  },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:05",
      "visibilityTimeout": "00:05:00",
      "batchSize": 16,
      "maxDequeueCount": 5,
      "newBatchThreshold": 8
    }
  }
}
